<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>common-metadata</artifactId>
        <groupId>mappings</groupId>
        <version>2018.12.01</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <artifactId>instance-types</artifactId>
    <groupId>mappings</groupId>
    <packaging>jar</packaging>
    <version>2018.12.01</version>


    <!--This is a module, it inherits all config properties form the parent pom.xml
You can configure here:
* the artifactId, please also change <module> in parent pom.xml
* the labels for your dataset (Everything behind the @ is used as language tag):
<labels>
    <label>Name in English@en</label>
    <label>Name in French@fr</label>
    ...
</labels>
* the description
-->

    <build>
        <plugins>
            <plugin>
                <groupId>org.dbpedia.databus</groupId>
                <artifactId>databus-maven-plugin</artifactId>
                <configuration>
                    <labels>
                        <label>Instance Types@en</label>
                    </labels>
                    <datasetDescription>
                        Contains triples of the form ($resource, http://www.w3.org/1999/02/22-rdf-syntax-ns#type,
                        $dbpedia_ontology-class) generated by the mappings extractor. The dataset contains a file with
                        just the types and one file with the pre-calculated, forward-inferences transitive closure.
                        If your application/store does not support reasoning, use the 'transitive' file.
                    </datasetDescription>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
